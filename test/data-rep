$at_quiet $as_echo_n " 61: $at_desc                               "
at_xfail=no
echo "#                             -*- compilation -*-" >> "$at_group_log"
(
  $as_echo "61. pointer.at:23: testing ..."
  $at_traceon


cat >prog.cob <<'_ATEOF'

       IDENTIFICATION   DIVISION.
       PROGRAM-ID.      prog.
       DATA             DIVISION.
       WORKING-STORAGE  SECTION.
       01 PTR           USAGE POINTER VALUE NULL.
       PROCEDURE        DIVISION.
           DISPLAY PTR
           END-DISPLAY.
           SET PTR UP BY 1
           DISPLAY PTR
           END-DISPLAY.
           STOP RUN.
_ATEOF


{ $at_traceoff
$as_echo "$at_srcdir/pointer.at:40: \${COMPILE} -o prog prog.cob"
echo pointer.at:40 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; ${COMPILE} -o prog prog.cob ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; ${COMPILE} -o prog prog.cob ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/pointer.at:40"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/pointer.at:41: ./prog"
echo pointer.at:41 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; ./prog ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; ./prog ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/pointer.at:41"

$at_failed && at_func_log_failure
$at_traceon; }


  $at_traceoff
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 | eval $at_tee_pipe
at_status=`cat "$at_status_file"`
#AT_STOP_61
#AT_START_62
# 62. compute.at:8: COMPUTE ADD-SUB-MUL-DIV
at_setup_line='compute.at:8'
at_desc="COMPUTE ADD-SUB-MUL-DIV"
$at_quiet $as_echo_n " 62: $at_desc                        "
at_xfail=no
echo "#                             -*- compilation -*-" >> "$at_group_log"
(
  $as_echo "62. compute.at:8: testing ..."
  $at_traceon




{ $at_traceoff
$as_echo "$at_srcdir/compute.at:12: \${COMPILE_MODULE}  \${TEMPLATE}/comp/*.cob -o ."
echo compute.at:12 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; ${COMPILE_MODULE}  ${TEMPLATE}/comp/*.cob -o . ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; ${COMPILE_MODULE}  ${TEMPLATE}/comp/*.cob -o . ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/compute.at:12"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/compute.at:13: cobcrun test_all > res"
echo compute.at:13 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; cobcrun test_all > res ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; cobcrun test_all > res ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/compute.at:13"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/compute.at:14: diff \${TEMPLATE}/comp/result.txt res"
echo compute.at:14 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; diff ${TEMPLATE}/comp/result.txt res ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; diff ${TEMPLATE}/comp/result.txt res ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/compute.at:14"

$at_failed && at_func_log_failure
$at_traceon; }


{ $at_traceoff
$as_echo "$at_srcdir/compute.at:16: \${COMPILE_MODULE}  -ffast-op \${TEMPLATE}/comp/*.cob -o ."
echo compute.at:16 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; ${COMPILE_MODULE}  -ffast-op ${TEMPLATE}/comp/*.cob -o . ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; ${COMPILE_MODULE}  -ffast-op ${TEMPLATE}/comp/*.cob -o . ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/compute.at:16"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/compute.at:17: cobcrun test_all > res"
echo compute.at:17 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; cobcrun test_all > res ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; cobcrun test_all > res ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/compute.at:17"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/compute.at:18: diff \${TEMPLATE}/comp/result.txt res"
echo compute.at:18 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; diff ${TEMPLATE}/comp/result.txt res ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; diff ${TEMPLATE}/comp/result.txt res ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/compute.at:18"

$at_failed && at_func_log_failure
$at_traceon; }



  $at_traceoff
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 | eval $at_tee_pipe
