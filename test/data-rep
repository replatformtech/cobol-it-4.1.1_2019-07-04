#include <stdio.h>
int dump (unsigned char *data);
int dump (unsigned char *data)
{
  int i;
  for (i = 0; i < 10; i++)
    printf ("%02x", data[i]);
  puts ("");
  return 0;
}
_ATEOF


{ $at_traceoff
$as_echo "$at_srcdir/packed.at:592: \${COMPILE_MODULE} dump.c"
echo packed.at:592 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; ${COMPILE_MODULE} dump.c ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; ${COMPILE_MODULE} dump.c ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/packed.at:592"

$at_failed && at_func_log_failure
$at_traceon; }


sed -e 's/@USAGE@/COMP-6/' ${TEMPLATE}/numeric-ns-dump.cob > prog.cob

{ $at_traceoff
$as_echo "$at_srcdir/packed.at:596: \${COMPILE} prog.cob"
echo packed.at:596 >"$at_check_line_file"

