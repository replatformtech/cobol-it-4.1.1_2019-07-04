cat >hexdump.c <<'_ATEOF'

#include <stdio.h>
int hexdump (unsigned char *data);
int hexdump (unsigned char *data)
{
  int i;
  for (i = 0; i < 8; i++)
    printf ("%02x", data[i]);
  puts ("");
  return 0;
}
_ATEOF


cat >prog.cob <<'_ATEOF'

       IDENTIFICATION DIVISION.
         PROGRAM-ID.             prog.
       DATA DIVISION.

       WORKING-STORAGE SECTION.
       01  NATIONAL-VAR              PIC N(10) NATIONAL.
       01  STRING-VAR                PIC X(100).
       PROCEDURE DIVISION.
     *-------------------------------------------------------------
       STEUER SECTION.
       ANFANG.
           MOVE     "XXX" TO NATIONAL-VAR.
           INSPECT  NATIONAL-VAR CONVERTING
                    NATIONAL-OF("X" 819) TO SPACE.
           CALL "hexdump" using NATIONAL-VAR.

           MOVE     "HALLO" TO STRING-VAR.
           MOVE FUNCTION NATIONAL-OF(STRING-VAR 819)
                TO NATIONAL-VAR.
           CALL "hexdump" using NATIONAL-VAR.

           MOVE     "HALLO" TO STRING-VAR.
           MOVE FUNCTION NATIONAL-OF(STRING-VAR 819)(2:1)
                TO NATIONAL-VAR.
           CALL "hexdump" using NATIONAL-VAR.

           MOVE     "HALLO" TO NATIONAL-VAR.
           MOVE FUNCTION DISPLAY-OF(NATIONAL-VAR 819)(2:1)
                TO STRING-VAR.
           CALL "hexdump" using STRING-VAR.

           MOVE     NATIONAL-OF("" 819) TO NATIONAL-VAR.
           CALL "hexdump" using NATIONAL-VAR.
           MOVE     FUNCTION DISPLAY-OF(NATIONAL-VAR 1208) TO
                    STRING-VAR.
           CALL "hexdump" using STRING-VAR.

           MOVE     FUNCTION DISPLAY-OF(NATIONAL-VAR 13488) TO
                    STRING-VAR.
           CALL "hexdump" using STRING-VAR.
           .
       ENDE.
           GOBACK.
_ATEOF


{ $at_traceoff
$as_echo "$at_srcdir/binary.at:1313: \${COMPILE_MODULE}  hexdump.c"
